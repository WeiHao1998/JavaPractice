<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>TestServletUser</servlet-name>
        <servlet-class>com.cy.TestServletUser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TestServletUser</servlet-name>
        <url-pattern>/TestServletUser</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DoorListServlet</servlet-name>
        <servlet-class>com.cy.DoorListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DoorListServlet</servlet-name>
        <url-pattern>/DoorListServlet</url-pattern>
    </servlet-mapping>
</web-app>